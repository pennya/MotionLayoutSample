            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintLeft_toLeftOf="parent"
            motion:layout_constraintRight_toLeftOf="@+id/view2"
            motion:layout_constraintHorizontal_chainStyle="packed"
            android:visibility="visible"/>

        <Constraint
            android:id="@+id/view2"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintLeft_toRightOf="@+id/view1"
            motion:layout_constraintRight_toLeftOf="@+id/view3"
            android:visibility="visible"/>

        <Constraint
            android:id="@+id/view3"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintLeft_toRightOf="@+id/view2"
            motion:layout_constraintRight_toLeftOf="@+id/view4"
            android:visibility="visible"/>

        <Constraint
            android:id="@+id/view4"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintLeft_toRightOf="@+id/view3"
            motion:layout_constraintRight_toRightOf="parent"
            android:visibility="visible"/>


        <Constraint
            android:id="@+id/view5"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toBottomOf="@+id/view1"
            motion:layout_constraintLeft_toLeftOf="parent"
            motion:layout_constraintRight_toLeftOf="@+id/view6"
            motion:layout_constraintHorizontal_chainStyle="packed"
            android:visibility="visible"/>

        <Constraint
            android:id="@+id/view6"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toBottomOf="@+id/view1"
            motion:layout_constraintLeft_toRightOf="@+id/view5"
            motion:layout_constraintRight_toLeftOf="@+id/view7"
            android:visibility="visible"/>

        <Constraint
            android:id="@+id/view7"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toBottomOf="@+id/view1"
            motion:layout_constraintLeft_toRightOf="@+id/view6"
            motion:layout_constraintRight_toLeftOf="@+id/view8"
            android:visibility="visible"/>

        <Constraint
            android:id="@+id/view8"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toBottomOf="@+id/view1"
            motion:layout_constraintLeft_toRightOf="@+id/view7"
            motion:layout_constraintRight_toRightOf="parent"
            android:visibility="visible"/>


        <Constraint
            android:id="@+id/view9"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toBottomOf="@+id/view5"
            motion:layout_constraintLeft_toLeftOf="parent"
            motion:layout_constraintRight_toLeftOf="@+id/view10"
            motion:layout_constraintHorizontal_chainStyle="packed"
            android:visibility="visible"/>

        <Constraint
            android:id="@+id/view10"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toBottomOf="@+id/view5"
            motion:layout_constraintLeft_toRightOf="@+id/view9"
            motion:layout_constraintRight_toLeftOf="@+id/view11"
            android:visibility="visible"/>

        <Constraint
            android:id="@+id/view11"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toBottomOf="@+id/view5"
            motion:layout_constraintLeft_toRightOf="@+id/view10"
            motion:layout_constraintRight_toLeftOf="@+id/view12"
            android:visibility="visible"/>

        <Constraint
            android:id="@+id/view12"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toBottomOf="@+id/view5"
            motion:layout_constraintLeft_toRightOf="@+id/view11"
            motion:layout_constraintRight_toRightOf="parent"
            android:visibility="visible"/>
    </ConstraintSet>
</MotionScene>