<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:motion="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <Transition
        motion:constraintSetStart="@id/start"
        motion:constraintSetEnd="@id/end"
        motion:motionInterpolator="linear">

        <OnSwipe
            motion:dragDirection="dragDown"
            motion:touchAnchorSide="bottom"/>

        <KeyFrameSet>

            <KeyPosition
                motion:framePosition="50"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/view5"
                motion:percentX="0.2"
                motion:percentY="1" />

            <KeyPosition
                motion:framePosition="50"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/view6"
                motion:percentX="0.2"
                motion:percentY="1" />

            <KeyPosition
                motion:framePosition="50"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/view7"
                motion:percentX="0.2"
                motion:percentY="1" />

            <KeyPosition
                motion:framePosition="50"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/view8"
                motion:percentX="0.2"
                motion:percentY="1" />

            <KeyPosition
                motion:framePosition="50"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/view9"
                motion:percentX="1"
                motion:percentY="1" />

            <KeyPosition
                motion:framePosition="50"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/view10"
                motion:percentX="1"
                motion:percentY="1" />

            <KeyPosition
                motion:framePosition="50"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/view11"
                motion:percentX="1"
                motion:percentY="1" />

            <KeyPosition
                motion:framePosition="50"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/view12"
                motion:percentX="1"
                motion:percentY="1" />

            <KeyAttribute
                android:alpha="0"
                motion:framePosition="70"
                motion:motionTarget="@+id/view9"/>

            <KeyAttribute
                android:alpha="0"
                motion:framePosition="70"
                motion:motionTarget="@+id/view10"/>

            <KeyAttribute
                android:alpha="0"
                motion:framePosition="70"
                motion:motionTarget="@+id/view11"/>


            <KeyAttribute
                android:alpha="0"
                motion:framePosition="70"
                motion:motionTarget="@+id/view12"/>

        </KeyFrameSet>

    </Transition>

    <ConstraintSet android:id="@+id/start">

        <Constraint
            android:id="@+id/view1"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintLeft_toLeftOf="parent"
            motion:layout_constraintRight_toLeftOf="@+id/view2"
            motion:layout_constraintHorizontal_chainStyle="packed"
            android:visibility="visible"/>

        <Constraint
            android:id="@+id/view2"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintLeft_toRightOf="@+id/view1"
            motion:layout_constraintRight_toLeftOf="@+id/view3"
            android:visibility="visible"/>

        <Constraint
            android:id="@+id/view3"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintLeft_toRightOf="@+id/view2"
            motion:layout_constraintRight_toLeftOf="@+id/view4"
            android:visibility="visible"/>

        <Constraint
            android:id="@+id/view4"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintLeft_toRightOf="@+id/view3"
            motion:layout_constraintRight_toLeftOf="@+id/view5"
            android:visibility="visible"/>

        <Constraint
            android:id="@+id/view5"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintLeft_toRightOf="@+id/view4"
            motion:layout_constraintRight_toLeftOf="@+id/view6"
            android:visibility="visible"/>

        <Constraint
            android:id="@+id/view6"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintLeft_toRightOf="@+id/view5"
            motion:layout_constraintRight_toLeftOf="@+id/view7"
            android:visibility="visible"/>

        <Constraint
            android:id="@+id/view7"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintLeft_toRightOf="@+id/view6"
            motion:layout_constraintRight_toLeftOf="@+id/view8"
            android:visibility="visible"/>

        <Constraint
            android:id="@+id/view8"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintLeft_toRightOf="@+id/view7"
            motion:layout_constraintRight_toRightOf="parent"
            android:visibility="visible"/>



        <Constraint
            android:id="@+id/view9"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toBottomOf="@+id/view5"
            motion:layout_constraintLeft_toLeftOf="parent"
            motion:layout_constraintRight_toLeftOf="@+id/view10"
            motion:layout_constraintHorizontal_chainStyle="packed"
            android:visibility="gone"/>

        <Constraint
            android:id="@+id/view10"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toBottomOf="@+id/view5"
            motion:layout_constraintLeft_toRightOf="@+id/view9"
            motion:layout_constraintRight_toLeftOf="@+id/view11"
            android:visibility="gone"/>

        <Constraint
            android:id="@+id/view11"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toBottomOf="@+id/view5"
            motion:layout_constraintLeft_toRightOf="@+id/view10"
            motion:layout_constraintRight_toLeftOf="@+id/view12"
            android:visibility="gone"/>

        <Constraint
            android:id="@+id/view12"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="8dp"
            motion:layout_constraintTop_toBottomOf="@+id/view5"
            motion:layout_constraintLeft_toRightOf="@+id/view11"
            motion:layout_constraintRight_toRightOf="parent"
            android:visibility="gone"/>


    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
